{
  "kind": "collectionType",
  "collectionName": "event_categories",
  "info": {
    "singularName": "event-category",
    "pluralName": "event-categories",
    "displayName": "event_category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "icon": {
      "allowedTypes": [
        "files",
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "color": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "inversedBy": "event_categories"
    }
  }
}
