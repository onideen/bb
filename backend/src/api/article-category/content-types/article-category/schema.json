{
  "kind": "collectionType",
  "collectionName": "article_categories",
  "info": {
    "singularName": "article-category",
    "pluralName": "article-categories",
    "displayName": "article_category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files",
        "images"
      ]
    },
    "color": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean"
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "inversedBy": "article_categories"
    }
  }
}
