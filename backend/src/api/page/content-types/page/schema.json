{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Side",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "external": {
      "type": "boolean",
      "default": false
    },
    "path": {
      "type": "string",
      "required": true
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.slider",
        "shared.rich-text",
        "shared.quote",
        "shared.media"
      ]
    },
    "slug": {
      "type": "string",
      "unique": false,
      "required": false
    },
    "navbar": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::navbar.navbar",
      "inversedBy": "pages"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "page.event-list",
        "page.article-list",
        "page.contact-list"
      ]
    }
  }
}
